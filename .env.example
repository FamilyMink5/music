# Discord Bot Token
DISCORD_BOT_TOKEN=your_token_here
DISCORD_CLIENT_ID=your_client_id_here

# Default Command Prefix
BOT_PREFIX=!

# PostgreSQL Database Connection URL
DATABASE_USER=postgres
DATABASE_PASSWORD=your_password_here
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=musicbot

# External Server SSH Connection Info
SSH_HOST=your_ssh_host
SSH_PORT=22
SSH_USERNAME=your_ssh_username
SSH_PRIVATE_KEY_PATH=path/to/private/key
SSH_YTDLP_PATH=/path/to/yt-dlp

# Local Config
LOCAL_YTDLP_PATH=./yt-dlp.exe

# Local NAS Path for Cache (Used as WebDAV target path)
NAS_CACHE_PATH=/path/to/cache/

# NAS WebDAV Credentials
NAS_WEBDAV_URL=http://your.webdav.server
NAS_WEBDAV_USERNAME=your_webdav_username
NAS_WEBDAV_PASSWORD=your_webdav_password

# Spotify API Credentials
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret 

# Music Bot Settings
MAX_SONG_DURATION_SECONDS=900
LOG_LEVEL=INFO 